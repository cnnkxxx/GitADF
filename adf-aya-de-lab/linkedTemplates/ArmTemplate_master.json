{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-aya-de-lab"
		},
		"AzureSQL_Rebrickable_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSQL_Rebrickable'"
		},
		"DL_Rebrickable_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DL_Rebrickable'"
		},
		"LS_AzureSQLDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AzureSQLDB'"
		},
		"AzureSQL_Rebrickable_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ayaanalytics.database.windows.net"
		},
		"AzureSQL_Rebrickable_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AyaAnalytics"
		},
		"AzureSQL_Rebrickable_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "KhineMyintMyat"
		},
		"DL_Rebrickable_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azdelab.dfs.core.windows.net/"
		},
		"HTTP_Rebrickable_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"LS_AzureSQLDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dbserverlab.database.windows.net"
		},
		"LS_AzureSQLDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "lab_db"
		},
		"LS_AzureSQLDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "demo"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-aya-de-lab_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSQL_Rebrickable_password": {
						"value": "[parameters('AzureSQL_Rebrickable_password')]"
					},
					"DL_Rebrickable_accountKey": {
						"value": "[parameters('DL_Rebrickable_accountKey')]"
					},
					"LS_AzureSQLDB_password": {
						"value": "[parameters('LS_AzureSQLDB_password')]"
					},
					"AzureSQL_Rebrickable_properties_typeProperties_server": {
						"value": "[parameters('AzureSQL_Rebrickable_properties_typeProperties_server')]"
					},
					"AzureSQL_Rebrickable_properties_typeProperties_database": {
						"value": "[parameters('AzureSQL_Rebrickable_properties_typeProperties_database')]"
					},
					"AzureSQL_Rebrickable_properties_typeProperties_userName": {
						"value": "[parameters('AzureSQL_Rebrickable_properties_typeProperties_userName')]"
					},
					"DL_Rebrickable_properties_typeProperties_url": {
						"value": "[parameters('DL_Rebrickable_properties_typeProperties_url')]"
					},
					"HTTP_Rebrickable_properties_typeProperties_url": {
						"value": "[parameters('HTTP_Rebrickable_properties_typeProperties_url')]"
					},
					"LS_AzureSQLDB_properties_typeProperties_server": {
						"value": "[parameters('LS_AzureSQLDB_properties_typeProperties_server')]"
					},
					"LS_AzureSQLDB_properties_typeProperties_database": {
						"value": "[parameters('LS_AzureSQLDB_properties_typeProperties_database')]"
					},
					"LS_AzureSQLDB_properties_typeProperties_userName": {
						"value": "[parameters('LS_AzureSQLDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-aya-de-lab_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-aya-de-lab_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf-aya-de-lab_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-aya-de-lab_ArmTemplate_1')]"
			]
		}
	]
}